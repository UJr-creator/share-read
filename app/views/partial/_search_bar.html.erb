<div class="search_bar">
  <div class="container">
    <%= form_tag(books_search_path, method: :get, class: 'form-inline') do %>
      <%= text_field_tag :title, @title, class: 'form-control', placeholder: '本のタイトル', size: 60 %>
      <%= submit_tag '商品を検索', class: 'btn'%>
    <% end %>
  </div>
</div>