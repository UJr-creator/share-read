<% if @books.present? %>
<div class="container">
  <div class="text-center">
    <div class="headline mb-5 mt-5">
      <p class="txt-l mb-1">『<%= @title %>』の検索結果</p>
    </div>
  </div>
  <%= render "partial/book_list", books: @books %>
</div>
<% else %>
  <div class="container text-center mt-5 mb-5">
    <p>検索結果はありませんでした。</p>
  </div>
<% end %>