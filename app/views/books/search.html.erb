<h1>Rakuserach</h1>
<div class='content'>
  <div id="search-box">
    <%= form_with url: books_search_path, method: :get, local: true do |f| %>
        <%= text_field_tag :keyword, "",  id: "good_search", name: "keyword", placeholder: "キーワードを打ち込んでください", style: "width: 200px;"%><button title="検索" type="submit">検索</button>
    <% end %>
  </div>
  <h2>検索結果</h2>
  <div>
    <%= render "partial/book_list" %>
  </div>
</div>